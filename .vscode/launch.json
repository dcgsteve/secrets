    "version": "0.2.0",
    "configurations": [
        {
            "name": "Empty launch",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}"
        },
        {
            "name": "Set secret",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": ["set","test1","hello and goodbye"]
        },
        {
            "name": "Get secret",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": ["get","test1"]
        },
        {
            "name": "Get missing secret",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": ["get","fsdkjhgjkdfshkghfdsjkghjkhdsfk"]
        },
        {
            "name": "Delete secret",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": ["delete","test1"]
        },
        {
            "name": "Show config",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": ["config","show"]
        },
        {
            "name": "List secrets",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": ["list"]
        },
    ]
}